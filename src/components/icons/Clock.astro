---
interface Props {
  size?: number;
  className?: string;
}

const { className, size = 24 } = Astro.props;
---

<svg
  xmlns="http://www.w3.org/2000/svg"
  width={size ?? 24}
  height={size ?? 24}
  viewBox="0 0 24 24"
  fill="none"
  stroke-width="1.3"
  stroke-linecap="round"
  stroke-linejoin="round"
  class={className}
>
  <circle cx="12" cy="12" r="10"> </circle>
  <polyline points="12 6 12 12 16 14"> </polyline>
</svg>
