---
interface Props {
  size?: number;
  className?: string;
}

const { className, size = 24 } = Astro.props;
---

<svg
  xmlns="http://www.w3.org/2000/svg"
  width={size ?? 24}
  height={size ?? 24}
  viewBox="0 0 24 24"
  fill="none"
  stroke-width="1.3"
  stroke-linecap="round"
  stroke-linejoin="round"
  class={className}
>
  <polygon points="6 3 20 12 6 21 6 3"> </polygon>
</svg>
